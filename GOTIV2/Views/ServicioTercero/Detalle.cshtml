@model GOTIV2.Models.sp_SPE_ServicioTercero_Listar_Result

@{
    ViewBag.Title = "Registrar Cierre de Digitación";
}

<p>&nbsp;</p>
<h4>Registrar Cierre de Digitación</h4>
<p>&nbsp;</p>


    <div class="form-horizontal">
        <div class="row form-group">
            <div class="col-md-2 control-label">Nro. Servicio</div>
            <div class="col-md-2">
                @Html.TextBox("ServicioTercero_Id", Model.ServicioTercero_Id, new { @class = "form control" })
            </div>
        </div>
        <div class="row form-group">
            <div class="col-md-2 control-label">Nro. Expediente</div>
            <div class="col-md-2">              
                @Html.TextBox("ExpedienteRequerimiento", Model.ExpedienteRequerimiento, new { @class = "form control" })
            </div>
        </div>

        <div class="row form-group">
            <div class="col-md-2 control-label">Orden de Servicio</div>
            <div class="col-md-2">
               
                @Html.TextBox("OrdenServicio", Model.OrdenServicio, new { @class = "form control" })
            </div>
        </div>

        <div class="row form-group">
            <div class="col-md-2 control-label">Fecha Orden de Servicio</div>
            <div class="col-md-2">
              
                @Html.TextBox("FechaOrdenServicio", Model.FechaOrdenServicio.Value.ToShortDateString(), new { @class = "form control" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-2 control-label">Personal</div>
            <div class="col-md-10">
         
                @Html.TextBox("NombreCompleto", Model.NombreCompleto, new { @class = "form control" })
            </div>
        </div>
        <div class="row form-group">
            <div class="col-md-2 control-label">Servicio Prestado</div>
            <div class="col-md-2">              
                @Html.TextBox("Servicio", Model.Servicio, new { @class = "form control" })
            </div>
        </div>
        <div class="row form-group">
            <div class="col-md-2 control-label">Producto Requerido</div>
            <div class="col-md-6">
                @Html.TextArea("ProductosRequeridos", Model.ProductosRequeridos, new { @class = "form control" })
            </div>
        </div>

        <div class="row form-group">
            <div class="col-md-2 control-label">Plazo Ejecución (Días)</div>
            <div class="col-md-1">
           
                @Html.TextBox("ProductosRequeridos", Model.PlazoEjecucion, new { @class = "form control" })
            </div>
        </div>
        <div class="row form-group">
            <div class="col-md-2 control-label">Monto</div>
            <div class="col-md-1">
             
                @Html.TextBox("ProductosRequeridos", Model.Monto, new { @class = "form control" })
            </div>
        </div>


</div>